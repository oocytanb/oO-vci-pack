      statsCache = {\n    --- \u6700\u5F8C\u306B\u7D71\u8A08\u60C5\u5831\u3092\u51FA\u529B\u3057\u305F\u6642\u9593\u3002\n   
      lastOutputTime = vci.me.UnscaledTime,\n\n    --- \u6295\u7403\u3092\u30AD\u30E3\u30C3\u30B7\u30E5\u3057\u3066\u3044\u308B\u6570\u3002\u3053\u306E\u5024\u304C\u30BC\u30ED\u3088\u308A\u5927\u304D\u3051\u308C\u3070\u3001\u9069\u5F53\u306A\u30BF\u30A4\u30DF\u30F3\u30B0\u3067\u7D71\u8A08\u60C5\u5831\u3092\u51FA\u529B\u3059\u308B\u3002\n   
