      then\n        -- \u81EA\u8EAB\u306E\u30E9\u30A4\u30C8\u304C\u5012\u308C\u3066\u3044\u308B\u72B6\u614B\u3067\u30D2\u30C3\u30C8\u3057\u305F\u5834\u5408\u306F\u3001\u7121\u8996\u3059\u308B\n       
      return\n    end\n\n    local deltaSec = (now - ls.inactiveTime).TotalSeconds\n   
      local velocity\n    if deltaSec > 0 then\n        local dp = li.GetPosition()
      - ls.respawnPosition\n        velocity = dp / deltaSec\n        cytanb.LogTrace('EmitHitStandLight:
      deltaPos = ', dp, ', deltaSec = ', deltaSec)\n    else\n        velocity =
      Vector3.zero\n        cytanb.LogTrace('EmitHitStandLight: zero velocity')\n   
      end\n\n    --@todo hitMessage \u304C\u6765\u308B\u524D\u306B\u3001\u30E9\u30A4\u30C8\u304C\u30BF\u30FC\u30B2\u30C3\u30C8\u306B\u5F53\u305F\u3063\u305F\u5834\u5408\u306F\u3001hitSourceID
      \u3092\u6B63\u3057\u304F\u8A2D\u5B9A\u3067\u304D\u306A\u3044\u306E\u3067\u3001\u5BFE\u7B56\u3092\u8003\u3048\u308B\u3002\n   
      local hitSourceID = now <= ls.hitMessageTime + settings.requestIntervalTime
