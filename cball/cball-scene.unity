      2) .. '), Vector3' .. tostring(stats.throwingAngularVelocity)\n        .. '\\n'
      .. indent .. 'predictedFlyingDistance: ' .. predictedFlyingDistanceString\nend\n\nlocal
      ProcessStatsCache = function ()\n    if statsCache.throwingCache <= 0 then\n       
      return\n    end\n\n    local now = vci.me.UnscaledTime\n    if now - settings.resetOperationTime
      <= statsCache.lastOutputTime then\n        return\n    end\n    statsCache.lastOutputTime
