      \u91CD\u529B\u51E6\u7406\n        local fixedSec = fixedTimestep.TotalSeconds\n       
      local timeRate = deltaSec / fixedSec\n        local accY\n        if ballStatus.gravityFactor
      > 0.0 and timeRate > 1.0 and velocity.y <= 0.0 then\n            -- \u4E0A\u5411\u304D\u306B\u529B\u3092\u52A0\u3048\u308B\u5834\u5408\u306F\u3001\u4E0B\u5411\u304D\u306B\u843D\u4E0B\u3059\u308B\u306F\u305A\u304C\u3001\u30D5\u30EC\u30FC\u30E0\u30EC\u30FC\u30C8\u306E\u6E1B\u5C11\u306B\u3088\u308B\u6D6E\u304D\u4E0A\u304C\u308A\u3092\u306A\u308B\u3079\u304F\u6291\u3048\u308B\n           
